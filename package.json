{
  "engines": {
    "node": ">=4",
    "npm": ">=3.3"
  },
  "name": "github-summary",
  "version": "0.1.0",
  "description": "",
  "main": "lib/index.js",
  "jsnext:main": "src/index.js",
  "files": [
    "dist",
    "lib",
    "src"
  ],
  "scripts": {
    "clean": "rimraf lib dist",
    "build:prod": "NODE_ENV=production webpack --config webpack.production.config.babel.js",
    "build:lib": "babel src -d lib",
    "build": "npm run build:lib && npm run build:prod",
    "start": "babel-node server/index.js",
    "lint": "eslint src",
    "mocha": "NODE_ENV=test mocha --require babel-register test/**/*_spec.js",
    "test": "npm run lint && npm run mocha",
    "test:watch": "npm run mocha -- -w",
    "prepublish": "npm run clean && npm run build"
  },
  "keywords": [
    "Github",
    "nippou",
    "ES6"
  ],
  "author": "zenoplex <zenoplex@gmail.com> (http://zenoplex.jp)",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.7.5",
    "babel-core": "^6.7.6",
    "babel-eslint": "^6.0.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-espower": "^2.1.2",
    "babel-plugin-transform-object-rest-spread": "^6.6.5",
    "babel-plugin-transform-runtime": "^6.7.5",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-0": "^6.3.13",
    "eslint": "^2.7.0",
    "eslint-config-airbnb": "^7.0.0",
    "eslint-plugin-babel": "^3.2.0",
    "eslint-plugin-jsx-a11y": "^0.6.2",
    "eslint-plugin-react": "^4.3.0",
    "espower-babel": "^4.0.3",
    "express": "^4.13.4",
    "html-webpack-plugin": "^2.15.0",
    "jsdom": "^8.3.1",
    "mocha": "^2.4.5",
    "path": "^0.12.7",
    "power-assert": "^1.3.1",
    "rimraf": "^2.5.2",
    "sinon": "^1.17.3",
    "webpack": "^1.12.15",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.10.0"
  },
  "dependencies": {
    "github-api": "^0.11.2",
    "node-github": "0.0.3",
    "octonode": "^0.7.5",
    "request": "^2.59.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/zenoplex/github-summary"
  }
}
