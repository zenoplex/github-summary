{
  "engines": {
    "node": ">=6",
    "npm": ">=3.3"
  },
  "name": "github-summary",
  "version": "0.0.1",
  "description": "",
  "main": "lib/index.js",
  "jsnext:main": "src/index.js",
  "files": [
    "dist",
    "lib",
    "src"
  ],
  "scripts": {
    "clean": "rimraf lib dist",
    "build:prod": "NODE_ENV=production webpack --config webpack.config.prod.js",
    "build:lib": "babel src -d lib",
    "build": "npm run build:lib && npm run build:prod",
    "start": "node server/index.js",
    "lint": "eslint src test",
    "mocha": "NODE_ENV=test mocha --compilers js:babel-register test/*_spec.js",
    "test": "npm run lint && npm run mocha",
    "test:watch": "npm run mocha -- -w",
    "prepublish": "npm run clean && npm run build"
  },
  "keywords": [
    "Github",
    "nippou",
    "summary",
    "ES6"
  ],
  "author": "zenoplex <zenoplex@gmail.com> (http://zenoplex.jp)",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.8.0",
    "babel-core": "^6.8.0",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-syntax-trailing-function-commas": "^6.8.0",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-plugin-transform-runtime": "^6.8.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-power-assert": "^1.0.0",
    "babel-register": "^6.8.0",
    "eslint": "^2.9.0",
    "eslint-config-airbnb-base": "^3.0.1",
    "eslint-import-resolver-webpack": "^0.2.4",
    "eslint-plugin-babel": "^3.2.0",
    "eslint-plugin-import": "^1.7.0",
    "express": "^4.13.4",
    "html-webpack-plugin": "^2.16.1",
    "jsdom": "^8.3.1",
    "mocha": "^2.4.5",
    "path": "^0.12.7",
    "power-assert": "^1.4.1",
    "rimraf": "^2.5.2",
    "sinon": "^1.17.4",
    "webpack": "^1.13.0",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.10.0"
  },
  "dependencies": {
    "axios": "^0.11.0",
    "buffer": "^4.6.0",
    "lodash": "^4.12.0",
    "parse-link-header": "^0.4.1",
    "to-markdown": "^3.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/zenoplex/github-summary"
  }
}
